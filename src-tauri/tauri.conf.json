{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "cadremanagementsystem",
  "version": "0.1.0",
  "identifier": "com.yven.cadremanagementsystem",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "cadremanagementsystem",
        "width": 1280,
        "height": 720,
        "minWidth": 1280,
        "minHeight": 720
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "dialog": null
  }
}
